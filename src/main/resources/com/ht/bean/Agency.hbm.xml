<hibernate-mapping package="com.ht.bean">
    <class name="Agency" table="t_agency">
        <id name="id" column="id">
            <generator class="uuid"/>
        </id>
        <property name="name" />
        <property name="email"/>
        <property name="pwd" />
        <property name="leader" />
        <property name="phone" />
        <property name="tel" />
        <property name="address" />
        <property name="logo" />
        <property name="intro" />
        <property update="false" name="created_time" />
        <property name="checked_status" />
        <property name="status" />

        <many-to-one update="false" name="role" column="role_id" class="Role"/>

