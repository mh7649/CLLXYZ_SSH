<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>


	<package name="buildings" namespace="/buildings" extends="base-package">
			<action name="*" class="buildingsController" method="{1}">
				<result name="success" type="json"/>
				<result name="adminBuildingsDetail">/admin/admin_buildings_detail.jsp</result>
				<result name="buildingsImg">/admin/admin_buildingsImg.jsp</result>
				<result name="showBuildings">/index_page/buildings/buildings.jsp</result>
				<result name="showBuildingsInfo">/index_page/buildings/buildings_info.jsp</result>
				<result name="showMap">/index_page/buildings/buildings_map.jsp</result>
				<result name="showSearchResults">/index_page/buildings/buildings_searchResult.jsp</result>
			</action>
	</package>

</struts>